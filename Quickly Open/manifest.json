{
    "name": "Quickly",
    "version": "1.0",
    "description": "Open Common URL",
    "permissions": [
        "bookmarks",
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_title": "Quickly Open"
    },
    "icons": {
        "19": "icon_19.png",
        "48": "icon_38.png",
        "128": "icon_38.png"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
